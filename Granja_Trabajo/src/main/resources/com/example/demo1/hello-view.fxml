<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.HelloController">
   <children>
      <HBox alignment="CENTER" spacing="10">
         <ImageView fitHeight="230.0" fitWidth="339.0">
            <image>
               <Image url="@../../../Imagenes/2024-04-26 16_10_58-alimentos y suministros de granja - Buscar con Google.png" />
            </image>
         </ImageView>
      </HBox>
      <StackPane alignment="CENTER">
         <ImageView fx:id="fotoPerfilView" fitHeight="131.0" fitWidth="139.0" />
         <ImageView fx:id="defaultImageView" fitHeight="115.0" fitWidth="118.0">
            <image>
               <Image url="@../../../Imagenes/6662120.png" />
            </image>
         </ImageView>
      </StackPane>
      <HBox alignment="CENTER" spacing="22.0">
         <Label text="Nombre" />
         <TextField fx:id="nombreTxtF" />
         <padding>
            <Insets left="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10">
         <Label text="Contraseña" />
         <PasswordField fx:id="contrasenyaTxtF" />
      </HBox>
      <HBox alignment="CENTER" spacing="10">
         <Button fx:id="botonLogIn" mnemonicParsing="false" onAction="#loginButton" prefHeight="64.0" prefWidth="116.0" text="Iniciar Sesion" />
         <Button fx:id="botonCerrarSesion" mnemonicParsing="false" onAction="#handleCerrarSesion" prefHeight="25.0" prefWidth="95.0" text="Cerrar Sesión" />
      </HBox>
   </children>
</VBox>
