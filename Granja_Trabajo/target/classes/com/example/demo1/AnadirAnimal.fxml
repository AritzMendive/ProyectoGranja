<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.AnadirAnimal">
    <children>
        <Button fx:id="mostrarAlimentos" layoutX="35.0" layoutY="320.0" mnemonicParsing="false" onAction="#mostrarAlimentos" text="Mostrar Alimentos" />
        <Button fx:id="editarBTN" layoutX="183.0" layoutY="320.0" mnemonicParsing="false" onAction="#editarStock" text="Editar Cantidad" />
        <Button fx:id="botonCerrarSesion" layoutX="559.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleCerrarSesion" prefHeight="26.0" prefWidth="26.0" text="X" />
        <Button fx:id="GestionBTN" layoutX="308.0" layoutY="320.0" mnemonicParsing="false" onAction="#gestionarAnimales" text="Gestionar Animales" />
        <Button layoutX="131.0" layoutY="51.0" onAction="#handleEditarPerfil" text="Cambio de perfil" />
        <ListView fx:id="AlimentacionListView" layoutX="35.0" layoutY="100.0" prefHeight="200.0" prefWidth="240.0" />
        <VBox alignment="TOP_CENTER" layoutX="292.0" layoutY="33.0" prefHeight="255.0" prefWidth="270.0" spacing="20.0">
            <children>
                <TextField fx:id="fieldNombre" alignment="CENTER" editable="false" style="-fx-background-color: transparent;" text="Cantidad a modificar">
                    <font>
                        <Font size="15.0" />
                    </font>
                </TextField>
                <TextField fx:id="fieldStock" alignment="CENTER" style="-fx-background-color: white;" text="Cantidad">
                    <font>
                        <Font size="15.0" />
                    </font>
                </TextField>
            </children>
        </VBox>
        <ImageView fx:id="fotoPerfilView" fitHeight="68.0" fitWidth="82.0" layoutX="21.0" layoutY="17.0" />
    </children>
</AnchorPane>
